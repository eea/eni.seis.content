<?xml version="1.0"?>
<object name="portal_skins">

    <object name="eni_seis" meta_type="Filesystem Directory View"
            directory="eni.seis.content:skins/eni_seis" />

    <object name="eni_tinymce_override" meta_type="Filesystem Directory View"
            directory="eni.seis.content:skins/eni_tinymce_override" />

    <object name="eni_faq_section"
            meta_type="Filesystem Directory View"
            directory="eni.seis.content:skins/eni_faq_section"/>

    <skin-path name="*">
        <layer name="eni_seis" insert-after="custom" />
        <layer name="eni_tinymce_override" insert-after="custom" />
        <layer name="eni_faq_section" insert-after="custom" />
    </skin-path>

</object>
