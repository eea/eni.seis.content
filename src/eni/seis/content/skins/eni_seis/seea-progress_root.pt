<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">

  <tal:col metal:fill-slot="portlets_one_slot"></tal:col>
  <tal:col metal:fill-slot="portlets_two_slot"></tal:col>

  <body metal:fill-slot="content-core">
    <div data-diazo="seea-content"
         tal:define="icons python: {'newsletters': {'is_fa': False, 'text': './newsletter-icon.svg'}, 'knowledge resources': {'is_fa': True, 'text': 'fa
          fa-file-pdf-o'}, 'visibility materials': {'is_fa': False, 'text':
          './leaflet-icon.svg'}}">

      <div class="site-container">
        <div class="seea-progress-container">

          <div class="site-container title-section">
            <div class="container">
              <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                  <h1 class="documentFirstHeading text-center"
                      tal:content="context/Title">
                    SEEA Progress
                  </h1>
                </div>
              </div>
            </div>
          </div>

          <div class="container seea-progress-section1">
            <div class="row vertical-align">
              <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 col-left">
                <img class="img-responsive lead-img" src="./seea-progress-01.png" />
              </div>
              <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                <div tal:replace="structure python:context.long_description">Body text</div>
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="site-container-fluid">
        <div class="container-fluid seea-progress-section2">
          <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
              <div class="knowledge-resources"
                   tal:define="contents python:context['knowledge-resources'].getFolderContents()[:6]"
                   tal:on-error="string:'ERROR: Missing folder knowledge-resources.'">

                <h3>Knowledge resources</h3>

                <ul class="list-group">
                  <li tal:repeat="brain python:contents"
                      class="list-group-item">
                    <span tal:define="
                      default_icon python:{'is_fa': True, 'text': 'fa
                      fa-file-o'};
                      icon python:icons.get('knowledge resources',
                      default_icon);
                      icon_is_fa python:icon.get('is_fa', True)">
                      <i tal:condition="python:icon_is_fa is True"
                         tal:attributes="class python:icon.get('text',
                         default_icon.get('text'))">
                      </i>
                      <img tal:condition="python:icon_is_fa is False"
                           tal:attributes="src python:icon.get('text', './leaflet-icon.svg')"/>
                    </span>
                    <a href="" tal:attributes="href brain/getURL">
                      <span tal:replace="brain/Title">Title here</span>
                    </a>
                  </li>
                </ul>

                <a href="./knowledge-resources" class="more-items pull-right">
                  More resources
                </a>
              </div>
            </div>
          </div>
        </div>
      </div>

